<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hurricaneapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="MaterialDesign"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.HurricaneApp">
        <activity android:name=".MaterialDesignPractice.TabBarLayoutActivity"></activity>
        <activity android:name=".MaterialDesignPractice.LoginPageActivity" />
        <activity
            android:name=".MaterialDesignPractice.CollapsingToolBarActivity"
            android:theme="@style/CollapsingToolbarActivityTheme" />
        <activity android:name=".MaterialDesignPractice.MaterialDesignPracticeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NativeSwitchFlutterTestActivity"
            android:label="NativeFlutterSwitch" />
        <activity
            android:name="io.flutter.embedding.android.FlutterActivity"
            android:configChanges="orientation|keyboardHidden|keyboard|screenSize|locale|layoutDirection|fontScale|screenLayout|density|uiMode"
            android:hardwareAccelerated="true"
            android:windowSoftInputMode="adjustResize" />
    </application>

</manifest>